{
  "manifest_version": 2,

  "name": "w205TwitterProject",
  "description": "This extension highlights spammy links",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Highlight suspicious content!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
